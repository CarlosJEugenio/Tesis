journal = {Pediatric critical care medicine : a journal of the Society of Critical Care Medicine and the World Federation of Pediatric Intensive and Critical Care Societies},
keywords = {Cardiac Output,Catheterization, Central Venous,Catheterization, Peripheral,Child,Extracorporeal Circulation,Hemodynamics,Humans,Infant, Newborn,Intensive Care Units, Pediatric,Models, Cardiovascular,Ultrasonics},
number = {4},
pages = {423--8},
pmid = {18496416},
title = {{Theory and in vitro validation of a new extracorporeal arteriovenous loop approach for hemodynamic assessment in pediatric and neonatal intensive care unit patients.}},
