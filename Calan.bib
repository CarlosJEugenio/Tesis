doi = {10.1097/01.PCC.0b013e31816c71bc},
file = {:home/nanosintesis/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Krivitski, Kislukhin, Thuramalla - 2008 - Theory and in vitro validation of a new extracorporeal arteriovenous loop approach for hemodyn.pdf:pdf},
isbn = {3540225722},
