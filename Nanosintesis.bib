@article{Lee2008,
abstract = {We measured the elastic properties and intrinsic breaking strength of free-standing monolayer graphene membranes by nanoindentation in an atomic force microscope. The force-displacement behavior is interpreted within a framework of nonlinear elastic stress-strain response, and yields second- and third-order elastic stiffnesses of 340 newtons per meter (N m−1) and −690 Nm−1, respectively. The breaking strength is 42 N m−1 and represents the intrinsic strength of a defect-free sheet. These quantities correspond to a Young's modulus of E = 1.0 terapascals, third-order elastic stiffness of D = –2.0 terapascals, and intrinsic strength of sint = 130 gigapascals for bulk graphite. These experiments establish graphene as the strongest material ever measured, and show that atomically perfect nanoscale materials can be mechanically tested to deformations well beyond the linear regime.},
archivePrefix = {arXiv},
arxivId = {47749150628},
author = {Lee, Changgu and Wei, Xiaoding and Kysar, Jeffrey W. and Hone, James and =},
doi = {10.1126/science.1157996},
eprint = {47749150628},
file = {:home/nanosintesis/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lee et al. - 2008 - Measurement of the Elastic Properties and Intrinsic Strength of Monolayer Graphene.pdf:pdf},
isbn = {0036-8075},
issn = {0036-8075},
journal = {Science},
number = {18 July 2008},
pages = {385--388},
pmid = {18635798},
title = {{Measurement of the Elastic Properties and Intrinsic Strength of Monolayer Graphene}},
volume = {321},
year = {2008}
}
