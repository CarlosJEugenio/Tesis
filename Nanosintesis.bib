issn = {1932-7447},
journal = {The Journal of Physical Chemistry C},
pages = {8993--8998},
title = {{Few-Layer Graphene-Encapsulated Metal Nanoparticles for Surface-Enhanced Raman Spectroscopy}},
url = {http://pubs.acs.org/doi/abs/10.1021/jp500751a},
volume = {118},
year = {2014}
}
@article{Xu2014,
