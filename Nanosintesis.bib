@article{Lee2013,
abstract = {We report the fabrication of graphene-encapsulated nanoballs with copper nanoparticle (Cu NP) cores whose size range from 40 nm to 1 $\mu$m using a solid carbon source of poly(methyl methacrylate) (PMMA). The Cu NPs were prone to agglomerate during the annealing process at high temperatures of 800 to 900 °C when gas carbon source such as methane was used for the growth of graphene. On the contrary, the morphologies of the Cu NPs were unchanged during the growth of graphene at the same temperature range when PMMA coating was used. The solid source of PMMA was first converted to amorphous carbon layers through a pyrolysis process at the temperature regime of 400 °C, which prevented the Cu NPs from agglomeration, and they were converted to few-layered graphene (FLG) at the elevated temperatures. Raman and transmission electron microscope analyses confirmed the synthesis of FLG with thickness of approximately 3 nm directly on the surface of the Cu NPs. X-ray diffraction and X-ray photoelectron spectroscopy analyses, along with electrical resistance measurement according to temperature changes showed that the FLG-encapsulated Cu NPs were highly resistant to oxidation even after exposure to severe oxidation conditions.},
author = {Lee, Sanggeun and Hong, Juree and Koo, Ja Hoon and Lee, Hyonik and Lee, Seulah and Choi, Taejin and Jung, Hanearl and Koo, Bonwoong and Park, Jusang and Kim, Hyungjun and Kim, Young Woon and Lee, Taeyoon},
doi = {10.1021/am3024965},
file = {:home/nanosintesis/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lee et al. - 2013 - Synthesis of few-layered graphene nanoballs with copper cores using solid carbon source.pdf:pdf},
isbn = {1944-8252 (Electronic)$\backslash$r1944-8244 (Linking)},
issn = {19448244},
journal = {ACS Applied Materials and Interfaces},
keywords = {copper nanoparticle,encapsulation,graphene,graphene nanoball},
number = {7},
pages = {2432--2437},
pmid = {23465382},
title = {{Synthesis of few-layered graphene nanoballs with copper cores using solid carbon source}},
volume = {5},
year = {2013}
}
