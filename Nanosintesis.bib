file = {:home/nanosintesis/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Pan et al. - 2014 - Noncovalently Functionalized Graphene-Directed Synthesis of Ultralarge Graphene-Based TiO 2 Nanosheet Composites Tun.pdf:pdf},
isbn = {1932-7447},
issn = {1932-7447},
journal = {The Journal of Physical Chemistry C},
number = {47},
pages = {27325--27335},
title = {{Noncovalently Functionalized Graphene-Directed Synthesis of Ultralarge Graphene-Based TiO 2 Nanosheet Composites: Tunable Morphology and Photocatalytic Applications}},
