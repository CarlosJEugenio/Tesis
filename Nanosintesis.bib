file = {:home/nanosintesis/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Xu et al. - 2014 - Highly efficient synthesis of neat graphene nanoscrolls from graphene oxide by well-controlled lyophilization.pdf:pdf},
issn = {15205002},
